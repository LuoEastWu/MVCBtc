<configuration>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />

    <!-- 连接字符串是否加密 -->
    <add key="ConStringEncrypt" value="false"/>
    <add key="FCKeditor:UserFilesPath" value="../Admin/UpFile/"/>
    <add key="FCKeditor:BasePath" value="../Admin/js/fckeditor/"/>
    <!-- 数据库连接字符串,（如果采用加密方式，上面一项要设置为true；加密工具，可在官方下载，
     如果使用明文这样server=127.0.0.1;database=.....,上面则设置为false。 -->
    <add key="ConnectionString" value="server=DESKTOP-QSILMG5;database=BtoC;uid=sa;pwd=123"/>
    <!--其它模块连接字符串，可以不断增加以便同一个项目支持连接多个数据库。如果没有，可以删除该行-->
    <add key="ConnectionString2" value="server=DESKTOP-QSILMG5;database=BtoC;uid=sa;pwd=123"/>
  </appSettings>
  <connectionStrings>
    <!--分页存储过程数据库链接语句-->
    <add name="SQLConnString" connectionString="server=狂派威震天\SQL2005;database=BtoC;uid=sa;pwd=sa;Max Pool Size=100;Min Pool Size=2" providerName="System.Data.SqlClient"/>
    <add name="SQLProfileConnString" connectionString="server=狂派威震天\SQL2005;database=BtoC;uid=sa;pwd=sa;Max Pool Size=100;Min Pool Size=2" providerName="System.Data.SqlClient"/>
    <!--分页存储过程数据库链接语句结束-->
  </connectionStrings>
  <system.web>
